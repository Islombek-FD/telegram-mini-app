{
  "name": "telegram-mini-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://islombek-fd.github.io/telegram-mini-app",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite --mode development",
    "staging": "vite --mode staging",
    "build": "tsc --noEmit && vite build --mode production",
    "build:staging": "tsc --noEmit && vite build --mode staging",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "@telegram-apps/react-router-integration": "^1.0.0",
    "@telegram-apps/sdk-react": "^1.0.0",
    "axios": "^1.7.3",
    "classnames": "^2.5.1",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.6.2",
    "react-query": "^3.39.3",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.23.0",
    "react-swipeable": "^7.0.1",
    "react-textarea-autosize": "^8.5.3",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-query": "^1.2.8",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.1",
    "gh-pages": "^6.1.1",
    "globals": "^15.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  }
